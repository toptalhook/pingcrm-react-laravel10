{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
      "php": "^7.2|^8.1",
      "laravel/framework": "^10.0",
      "laravel/tinker": "^2.8.0",
      "inertiajs/inertia-laravel": "^0.6.9",
      "laravel/ui": "^4.2.1",
      "reinink/remember-query-strings": "^0.1.2",
      "sentry/sentry-laravel": "^3.2.0",
      "tightenco/ziggy": "^1.5.0"
    },
    "require-dev": {
      "fakerphp/faker": "^1.21.0",
      "mockery/mockery": "^1.5.1",
      "spatie/laravel-ignition": "^2.0.0",
      "nunomaduro/collision": "^7.0.5",
      "barryvdh/laravel-debugbar": "^3.8.0",
      "beyondcode/laravel-dump-server": "^1.9.0",
      "phpunit/phpunit": "^10.0",
      "brianium/paratest": "^7.0.6"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "compile": [
            "npm run prod",
            "@php artisan migrate:fresh --force",
            "@php artisan db:seed --force"
        ],
        "reseed": [
            "@php artisan migrate:fresh",
            "@php artisan db:seed"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
